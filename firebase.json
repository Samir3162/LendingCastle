{
  "hosting": {
    "public": "build/es6-unbundled",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
	  {
	    "source": "!/__/**",
	    "destination": "/index.html"
	  },
	  {
	    "source": "**/!(*.js|*.html|*.css|*.json|*.svg|*.png|*.jpg|*.jpeg|*.ico)",
	    "destination": "/index.html"
	  }
	]
  }
}
